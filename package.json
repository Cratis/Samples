{
    "private": true,
    "workspaces": [
        "Library"
    ],
    "engines": {
        "node": ">=23.0.0"
    },
    "scripts": {
        "g:clean": "cd $INIT_CWD && tsc -b --clean",
        "g:build": "cd $INIT_CWD && tsc -b && rollup -c ./rollup.config.mjs",
        "g:lint": "cd $INIT_CWD && eslint --cache --quiet --fix",
        "g:lint:ci": "cd $INIT_CWD && eslint --cache --quiet",
        "g:test": "cd $INIT_CWD && vitest run",
        "g:test:watch": "cd $INIT_CWD && vitest --watch",
        "g:test:debug": "DEBUG=vite:proxy && cd $INIT_CWD && vitest --watch",
        "g:ci": "cd $INIT_CWD && yarn clean && yarn lint:ci && yarn build && yarn test",
        "g:update": "cd $INIT_CWD && ncu -u && yarn",
        "g:preview": "cd $INIT_CWD && vite preview",
        "g:dev": "cd $INIT_CWD && vite --force",
        "g:dev:debug": "DEBUG=vite:proxy && cd $INIT_CWD && vite --force",
        "clean": "node ./run-task-on-workspaces.js clean",
        "build": "node ./run-task-on-workspaces.js build",
        "lint": "node ./run-task-on-workspaces.js lint",
        "test": "node ./run-task-on-workspaces.js test",
        "ci": "node ./run-task-on-workspaces.js ci",
        "publish-version": "node ./run-task-on-workspaces.js publish-version $*",
        "update": "node ./run-task-on-workspaces.js update"
    },
    "devDependencies": {
        "@eslint/eslintrc": "3.3.1",
        "@eslint/js": "9.24.0",
        "@types/chai": "5.2.1",
        "@types/chai-as-promised": "8.0.2",
        "@types/mocha": "10.0.10",
        "@types/node": "22.14.0",
        "@types/sinon": "17.0.4",
        "@types/sinon-chai": "4.0.0",
        "@typescript-eslint/eslint-plugin": "8.29.1",
        "@typescript-eslint/parser": "8.29.1",
        "@vitejs/plugin-react": "4.3.4",
        "@vitest/coverage-v8": "3.1.1",
        "autoprefixer": "10.4.21",
        "chai": "5.2.0",
        "chai-as-promised": "8.0.1",
        "edit-json-file": "1.8.1",
        "eslint": "9.24.0",
        "eslint-config-prettier": "10.1.1",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-no-null": "1.0.2",
        "eslint-plugin-prettier": "5.2.6",
        "eslint-plugin-react": "7.37.5",
        "glob": "11.0.1",
        "globals": "16.0.0",
        "mocha": "11.1.0",
        "module-alias": "2.2.3",
        "npm-check-updates": "17.1.16",
        "postcss": "8.5.3",
        "prettier": "3.5.3",
        "rollup": "4.39.0",
        "rollup-plugin-commonjs": "10.1.0",
        "rollup-plugin-peer-deps-external": "2.2.4",
        "rollup-plugin-typescript2": "0.36.0",
        "sinon": "20.0.0",
        "sinon-chai": "4.0.0",
        "tailwindcss": "3.4.17",
        "ts-node": "10.9.2",
        "typescript": "5.8.3",
        "typescript-eslint": "8.29.1",
        "vite": "6.2.5",
        "vite-plugin-commonjs": "0.10.4",
        "vite-tsconfig-paths": "5.1.4",
        "vitest": "3.1.1"
    },
    "license": "MIT",
    "version": "0.0.0",
    "packageManager": "yarn@4.5.3"
}

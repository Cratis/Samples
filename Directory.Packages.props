<Project>
  <PropertyGroup>
    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
    <Fundamentals>6.3.2</Fundamentals>
    <Applications>15.10.0</Applications>
    <Chronicle>11.13.4</Chronicle>
  </PropertyGroup>
  <ItemGroup>
    <PackageVersion Include="Cratis.Fundamentals" Version="$(Fundamentals)" />
